      },
      "API_ID": {
        "description": "Bu değeri şuradan alın: https://my.telegram.org",
        "value": "",
        "required": true
      },
      "API_HASH": {
        "description": "Bu değeri şuradan alın: https://my.telegram.org",
        "value": "",
        "required": true
      },
      "STRING_SESSION": {
        "description": "Python3 StringSessionOlustur.py'yi yerel olarak çalıştırarak bu değeri oluşturun..",
        "value": "",
        "required": true
      },
      "SESSION_ADI": {
        "description": "Botunuza isim verin (fark etmez)",
        "value": "ThorUserbot",
        "required": true
